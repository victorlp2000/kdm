<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragment_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/cehuabeijing">

    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <ImageView
            android:id="@+id/iv_cehua_touxiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="58dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/logo1_cehua" />

        <TextView
            android:id="@+id/tv_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@+id/iv_cehua_touxiang"
            android:text="@string/nichen"
            android:textColor="#ffffff"
            android:textSize="14sp"

            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_user"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_title"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_touxiangs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="9dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/touxiang_cehua" />

        <TextView
            android:id="@+id/tv_users"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_touxiangs"
            android:layout_marginLeft="11dp"
            android:layout_toRightOf="@+id/iv_cehua_touxiangs"
            android:padding="8dp"
            android:text="@string/yonghumingce"
            android:textColor="#ffffff"
            android:textSize="16sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_yibiaopan"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_user"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_yibiaopan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/yiqiyibiao" />

        <TextView
            android:id="@+id/tv_yibiaopan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_yibiaopan"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_yibiaopan"
            android:padding="8dp"
            android:text="@string/yibiaopan"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_yibiaoqi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_celiang"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_yibiaopan"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_celiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/celiang" />

        <TextView
            android:id="@+id/tv_celiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_celiang"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_celiang"
            android:padding="8dp"
            android:text="@string/celiang"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_celiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_jiatingchenyuan"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_celiang"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_jiatingceyuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/jiatingchengyuan" />

        <TextView
            android:id="@+id/tv_jaitingchengyuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_jiatingceyuan"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_jiatingceyuan"
            android:padding="8dp"
            android:text="@string/jaitingchengyuan"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_jaitingchengyuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_yishengziliao"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_jiatingchenyuan"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_yishengziliao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/yishengziliao" />

        <TextView
            android:id="@+id/tv_yishengziliao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_yishengziliao"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_yishengziliao"
            android:padding="8dp"
            android:text="@string/yishengziliao"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_yishengziliao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_shebeiliebiao"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_yishengziliao"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_shebeiliebiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/shebeiliebiao" />

        <TextView
            android:id="@+id/tv_shebeiliebiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_shebeiliebiao"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_shebeiliebiao"
            android:padding="8dp"
            android:text="@string/shebeiliebiao"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_sebeiliebiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_shezhi"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/rl_shebeiliebiao"
        android:layout_marginTop="6dp"
        android:background="#30FFFFFF">

        <ImageView
            android:id="@+id/iv_cehua_shezhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            android:src="@mipmap/shezhi" />

        <TextView
            android:id="@+id/tv_shezhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/iv_cehua_shezhi"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/iv_cehua_shezhi"
            android:padding="8dp"
            android:text="@string/shezhi"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_shezhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@mipmap/zhankai_2" />
    </RelativeLayout>



        <Button
            android:id="@+id/btn_zhuxiaodenglv"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp"
            android:background="@drawable/zhuxiaodenglv_shape"
            android:text="@string/zhuxiaodenglv"
            android:textSize="16sp"
            android:layout_below="@+id/rl_shezhi"
            android:layout_marginTop="14dp"
            />


</RelativeLayout>
